@model bsk2v2.ViewModels.UserListViewModel

<p>
    @Html.ActionLink("Add new user", "Create", "Account")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Users.FirstOrDefault().Username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.FirstOrDefault().CleranceLevel)
        </th>
        <th>
            Actions
        </th>
    </tr>

    @foreach (var user in Model.Users)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => user.Username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => user.CleranceLevel)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = user.Id }) |
                @Html.ActionLink("Details", "Details", new { id = user.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = user.Id })
            </td>
        </tr>
    }
</table>